{
  "name": "airbnb-ssr",
  "private": true,
  "version": "0.0.1",
  "author": "Leo Zeng",
  "scripts": {
    "dev:ssr": "cross-env NODE_ENV=development npx node-dev server.js",
    "prod:ssr": "cross-env NODE_ENV=production npx node-dev server.js",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "preview": "vite preview --host",
    "eslint": "eslint --ext .ts,.js,.vue,.json --ignore-path .gitignore --fix src",
    "prettier": "prettier --write .",
    "commit": "cz"
  },
  "dependencies": {
    "@nuxt/devalue": "^2.0.0",
    "@vueuse/core": "^8.9.0",
    "@vueuse/head": "^0.7.6",
    "axios": "^0.27.2",
    "element-plus": "^2.2.8",
    "express": "^4.18.1",
    "pinia": "^2.0.14",
    "uuid": "^8.3.2",
    "vite-ssr": "^0.16.0",
    "vue": "^3.2.37",
    "vue-i18n": "9",
    "vue-router": "^4.1.1"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.1.6",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/node": "^18.0.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vitejs/plugin-vue": "^2.3.1",
    "autoprefixer": "^10.4.4",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.2.0",
    "postcss": "^8.4.12",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.53.0",
    "serve-static": "^1.15.0",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.8.6",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.13",
    "vite-plugin-inspect": "^0.5.0",
    "vite-svg-loader": "^3.4.0",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.37.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
